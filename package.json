    "@mui/x-date-pickers": "^6.18.3",
    "ccusage": "^15.8.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.10.3",
    "uuid": "^9.0.1"
