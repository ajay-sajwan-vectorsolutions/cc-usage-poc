{
  "name": "claude-usage-tracker",
  "version": "1.0.0",
  "description": "Real-Time Claude Usage Tracker with automatic data collection",
  "homepage": "https://ajay-sajwan-vectorsolutions.github.io/cc-usage-poc",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode development --hot",
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development --hot",
    "server": "node server.js",
    "auto": "npm run build && npm run server",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "clean": "rimraf dist node_modules/.cache",
    "predeploy": "npm run build && cp public/sample-*.json dist/",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [
    "claude",
    "usage",
    "tracker",
    "analytics"
  ],
